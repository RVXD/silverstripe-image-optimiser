{
	"name": "axllent/silverstripe-image-optimiser",
	"description": "Automatically optimise both uploaded & resampled images for SilverStripe",
	"type": "silverstripe-vendormodule",
	"homepage": "https://github.com/axllent/silverstripe-image-optimiser",
	"keywords": ["silverstripe", "upload", "image", "compress", "optimisation"],
	"license": "MIT",
	"authors": [
		{
			"name": "Ralph Slooten",
			"homepage" : "http://www.axllent.org/"
		}
	],
	"support": {
		"issues": "https://github.com/axllent/silverstripe-image-optimiser/issues"
	},
    "require": {
        "silverstripe/assets": "^1.3.0",
        "spatie/image-optimizer": "^1.4"
    },
    "suggest": {
        "axllent/silverstripe-scaled-uploads": "Automatically scale down large uploaded images"
    },
    "autoload": {
        "psr-4": {
            "Axllent\\ImageOptimiser\\": "src/"
        }
    }
}
